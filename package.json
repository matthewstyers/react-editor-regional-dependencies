{
  "name": "regional-node-modules",
  "version": "0.0.5",
  "description": "",
  "repository": "git@github.com:matthewstyers/react-editor-regional-dependencies.git",
  "author": "Matthew Styers",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ../node_modules; rm -f ../package.json package-lock.json",
    "preupdate": "npm run clean && npm i -g npm-check-updates",
    "update": "ncu -u; npm i",
    "postupdate": "mkdir ../node_modules; mv ./node_modules/ ../; cp -a ./package.json ../",
    "update:dry": "ncu"
  },
  "dependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "2.0.0",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-env": "2.0.0-alpha.7",
    "eslint": "4.1.1",
    "eslint-config-keystone": "3.0.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-react-native": "2.3.2",
    "flow-bin": "0.49.1"
  }
}
