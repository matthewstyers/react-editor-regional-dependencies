    "postupdate": "mkdir ../node_modules; mv ./node_modules/ ../; cp -a ./package.json ../",
    "update-dry": "ncu"
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "2.0.0-alpha.7",
