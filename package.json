    },
    "dependencies": {
        "babel-core": "6.7.7",
        "babel-eslint": "6.0.4",
        "babel-preset-es2015": "6.6.0",
        "babel-preset-react": "6.5.0",
        "babel-preset-stage-1": "6.5.0",
        "eslint-plugin-react": "5.0.1",
        "eslint": "2.8.0",
        "react": "15.0.1",
        "react-addons-test-utils": "15.0.1"
    },
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-install": "^0.6.0"
    }
}
